{
  "name": "frans-client",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && go build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:go": "go run main.go",
    "services": "docker compose -f docker-compose.dev.yaml up",
    "keycloak:export": "./keycloak/keycloak_export.sh",
    "ent": "go run -mod=mod entgo.io/ent/cmd/ent",
    "ent:generate": "go run -mod=mod entgo.io/ent/cmd/ent generate ./pkg/ent/schema",
    "ent:new": "go run -mod=mod entgo.io/ent/cmd/ent new --target ./pkg/ent/schema"
  },
  "dependencies": {
    "@mantine/core": "^8.1.1",
    "@mantine/dropzone": "^8.1.1",
    "@mantine/form": "^8.1.1",
    "@mantine/hooks": "^8.1.1",
    "@tanstack/react-query": "^5.80.10",
    "@tanstack/react-router": "^1.121.21",
    "@tanstack/react-router-devtools": "^1.121.21",
    "axios": "^1.10.0",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.5.3",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@tanstack/react-query-devtools": "^5.80.10",
    "@tanstack/router-plugin": "^1.121.22",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.1",
    "eslint": "^9.28.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsx": "^4.19.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.3.5",
    "vite-plugin-i18next-loader": "^3.1.2"
  },
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": ">22.0.0"
  }
}
